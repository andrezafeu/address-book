<ul>
	<% @contacts.each do |contact| %>
		<li>
		<%= link_to contact.name, contact_path(contact) %>
			<%= contact.address %>
			<%= contact.phone %>
			<%= contact.email %>
			<%= link_to 'Delete', contact_path(contact.id), dta: { confirm: "Are you sure?" }, method: :delete %>
		</li>
	<% end %>
</ul>

<%= link_to 'Add a new contact', new_contact_path %>